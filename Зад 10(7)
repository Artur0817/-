#include <iostream>
#include <math.h>
using namespace std;
int main()
{
	setlocale(LC_ALL, "Russian");
	int a, b, c;
	cout << "Сторона треугольника а: ";
	cin >> a;
	cout << "Сторона треугольника b: ";
	cin >> b;
	cout << "Сторона треугольника с ";
	cin >> c;
	if (a + b > c && c + b > a && a + c > b)
	{

		cout << "Треугольник существует";
	}
	else
	{

		cout << "Треугольник не существует";
	}
	return 0;
}
