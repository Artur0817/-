#include <iostream>
using namespace std;
int a, b, c, ac, bc;
int main()
{
	setlocale(LC_ALL, "Russian");
	cin >> a;
	cout << " a = ";
	cin >> b;
	cout << " b = ";
	cin >> c;
	cout << " c= ";
	if ((a < c < b) and (b < c < a))
		cout << "ac = " << a * c;
	else 
		cout << "Введите другое c";
	if ((a < c < b) and (b < c < a))
		cout << "bc = " << b * c;
	else 
		cout << "Введите другое c";
	if ((a < c < b) and (b < c < a))
		cout << "Произведение = " << ac * bc;
	else 
		cout << "Введите другое c";
}
