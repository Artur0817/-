#include <iostream>
using namespace std;
int a, b, c, ac, bc;
int main() 
{

	setlocale(LC_ALL, "Russian");
	cin >> a;
	cout << " a = ";
	cin >> b;
	cout << " b = ";
	cin >> c;
	cout << " c= ";
	if 
		((b < c < a) or (a < c < a))
	    cout << "ac = " << a * c ;
	    cout << "bc = " << b * c;
		cout << "Произведение = ";
	else
		cout << "Введите другое c";
}
