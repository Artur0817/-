#include <iostream>
using namespace std;
int a, b, c, ac, bc;
int main()
{
	setlocale(LC_ALL, "Russian");
	cout << " a = ";
	cin >> a;
	cout << " b = ";
	cin >> b;
	cout << " c= ";
	cin >> c;
	if ((a < c < b) or (b < c < a))
	{
		cout << "ac = " << a * c << "\n";
		cout << "bc = " << b * c << "\n";
		cout << "Произведение = " << ac * bc << "\n";
	}
	else
		cout << "Введите другое c";
}
