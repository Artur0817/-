#include <iostream>
#include <math.h>
using namespace std;
int main()
{
	setlocale(LC_ALL, "Russian");
	int a, b, c, d, f;
	cout << "a=";
	cin >> a;
	b = a / 100;
	c = a % 100;
	d = c / 10;
	f = c % 10;
	if (b<d && d<f || b>d && d>f)
	{
		cout << "Высказывание истинно!";
	}
	else
	{
		cout << "Высказывание ложно!";
	}
	return 0;
}

