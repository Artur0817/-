#include <iostream>
#include <math.h>
using namespace std;
int main()
{
    setlocale(LC_ALL, "Russian");
    int a, b, c, s;
    cout << "a:";
    cin >> a;
    cout << "b:";
    cin >> b;
    cout << "c:";
    cin >> c;
    if (a < b && b < c)
    {
        s = b + c;
        cout << "Сумма наибольших чисел равна:" << s;
    }
    else
    {
        if (b < a && a < c)
        {
            s = a + c;
            cout << "Сумма наибольших чисел равна:" << s;
        }
        else
        {
            s = a + b;
            cout << "Сумма наибольших чисел равна:" << s;
        }
    }
    return 0;
}
