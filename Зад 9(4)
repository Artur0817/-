#include <iostream>
#include <math.h>
using namespace std;
int main()
{
	setlocale(LC_ALL, "Russian");
	int a, b, c, k, n;
	cout << "a:";
	cin >> a;
	cout << "b:";
	cin >> b;
	cout << "c:";
	cin >> c;
	k = (a / c) * (b / c);
	n = (a * b) - (k * c * c);
	cout << "Количество квадратов, размещенных на прямоугольнике:" << k;
	cout << "Площадь незанятой части прямоугольника:" << n;
	return 0;
}
