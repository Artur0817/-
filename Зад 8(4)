#include <iostream>
#include <math.h>
using namespace std;
int main() 
{
    setlocale(LC_ALL, "Russian");
    int a, b;
    cout << "a=";
    cin >> a;
    if (9 < a and a < 100)
    {
        b = (a / 10) + (a % 10) * 10;
        cout << "Обратное число =" << b << endl;
    }
    else
    {
        cout << "Введите другое число!";
    }
    return 0;
}
