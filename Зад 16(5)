#include<iostream>
#include<math.h>
using namespace std;
int main()
{
	setlocale(LC_ALL, "RUS");
	int N;
	cout << "Введите N " << endl;
	cin >> N;
	cout << "Введите массив A" << endl;
	int* A = new int[N];
	for (int i = 0; i < N; i++)
		cin >> A[i];
	
	for (int i = 0; i < N; i++) 
  {
		if (i/2==0)
		cout << A[i] << " ";
		
	}
	for (int i = N-1; i >=0; i--)
  {
		if (i / 2 != 0)
			cout << A[i]<<" ";
	}
	return 0;
}
