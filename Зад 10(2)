#include <iostream>
#include <math.h>
using namespace std;
int main()
{
	setlocale(LC_ALL, "Russian");
	int a, b, c;
	cout << "a=";
	cin >> a;
	cout << "b=";
	cin >> b;
	cout << "c=";
	cin >> c;
	if (a<b && b<c)
	{
		cout << "Высказывание истинно!";
	}
	else
	{
		cout << "Высказывание ложно!";
	}
	return 0;
}
