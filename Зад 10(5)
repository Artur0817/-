#include <iostream>
#include <math.h>
using namespace std;
int main()
{
	setlocale(LC_ALL, "Russian");
	int d, a, b;
	cout << "1-ый угол:";
	cin >> d;
	cout << "2-ой угол:";
	cin >> a;
	cout << "3-ий угол:";
	cin >> b;
	if (d + a + b == 180)
	{
		if (d == 90 || a == 90 || b == 90)
		{
			cout << "Треугольник является прямоугольным!";
		}
		else
		{
			cout << "Треугольник не является прямоугольным!";
		}
	}
	else
	{
		cout << "Не является треугольником!";
	}
	return 0;
}
