#include <iostream>
#include <math.h>
using namespace std;
int main() 
{
	setlocale(LC_ALL, "Russian");
	int a, b;
	cout << "a: ";
	cin >> a;
	if (99 < a and a < 1000)
	{
		b = (a / 100) + (a % 100) * 10;
		cout << "Полученное число: " << b << endl;
	}
	else
	{
		cout << "Введите другое число!";
	}
	return 0;
}
