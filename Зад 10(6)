#include <iostream>
#include <math.h>
using namespace std;
int main()
{
	setlocale(LC_ALL, "Russian");
	int d, e, f;
	cout << "1-ый угол:";
	cin >> d;
	cout << "2-ой угол:";
	cin >> e;
	cout << "3-ий угол:";
	cin >> f;
	if (d + e + f == 180)
	{
		if (d == 90 || e == 90 || f == 90)
		{
			cout << "Треугольник является прямоугольным!";
		}
		else
		{
			cout << "Треугольник не является прямоугольным!";
		}
	}
	else
	{
		cout << "Не является треугольником!";
	}
	return 0;
}
